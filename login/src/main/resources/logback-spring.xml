<?xml version="1.0" encoding="UTF-8"?><configuration>  <appender name="consoleAppender" class="ch.qos.logback.core.ConsoleAppender">    <encoder>      <pattern>[%d{yyyy.MM.dd HH:mm:ss.SSS}] - [%-5level] - [%X{requestId}] - [%logger{5}] - %msg%n</pattern>    </encoder>  </appender>  <root level="info">    <appender-ref ref="consoleAppender"/>  </root></configuration>